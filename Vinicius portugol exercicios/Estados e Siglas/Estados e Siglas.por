programa {
  funcao inicio() {
   cadeia sigla
  
   escreva ("Digite uma sigla de um estado: ")
    leia (sigla)

  escolha (sigla){
    caso "RJ": 
    escreva("Rio de Janeiro")
      pare
    caso "PE": 
    escreva("Pernambuco")
      pare
    caso "RN": 
    escreva("Rio Grande do Norte")
    pare
    caso contrario:
    escreva("Digite uma das siglas cadastradas")
   }
  }
}
